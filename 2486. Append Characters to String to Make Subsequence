Time complexity : O(n)
Space complexity : O(1)

class Solution {
    public int appendCharacters(String s, String t) {
        int j=0;
        int matching = 0;
        for(int i=0;i<s.length();i++){
            if(j < t.length() && s.charAt(i) == t.charAt(j)){
                matching++;
                j++;
            }
        }
        return t.length()-matching;
    }
}
